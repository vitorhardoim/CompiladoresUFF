var fibonacci = 0 ;

def fun_fib (num) {
    var cur = 1 ;
    var prev = 0;
    var cont = 1;
    var tmp = 0;
    while(cont < num){
        tmp := cur ;
        cur := cur + prev ;
        prev := tmp ;
        cont := cont + 1
    };
    return cur
}

fibonacci := fun_fib(20) ;
print(fibonacci)     #Output = [6765]